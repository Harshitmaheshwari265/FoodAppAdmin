<!-- Wappler include head-page="layouts/main" fontawesome_5="cdn" bootstrap5="local" is="dmx-app" id="usertype" appConnect="local" components="{dmxBootstrap5Navigation:{},dmxBootstrap5TableGenerator:{},dmxFormatter:{}}" -->
<meta name="ac:route" content="/user-type">
<dmx-serverconnect id="scUserTypeList" url="/api/Users/GetUserType"></dmx-serverconnect>

<div class="breadcome-area">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="breadcome-list">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="breadcomb-wp">
                                <div class="breadcomb-icon">
                                    <i class="icon nalika-user"></i>
                                </div>
                                <div class="breadcomb-ctn">
                                    <h2>Different User Types</h2>
                                    <p>Total: <span class="bread-ntd panel-heading" dmx-text="content.scUserTypeList.data.UserTypeList.data.length"></span></p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="product-status mg-b-30">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="product-status-wrap">
                    <h4>User Type List</h4>
                    <table>
                        <thead>
                            <tr>
                                <th>#</th>
                                <th>Type Name</th>
                                <th>Type Code</th>
                                <th>Status</th>
                                <th>Created By</th>
                                <th>Created On</th>
                            </tr>
                        </thead>
                        <tbody is="dmx-repeat" dmx-generator="bs5table" dmx-bind:repeat="scUserTypeList.data.UserTypeList.data" id="tableRepeat">
                            <tr>
                                <td>{{$index+1}}</td>
                                <td dmx-text="type_name"></td>
                                <td dmx-text="type_code"></td>
                                <td><button dmx-class:pd-setting="status==1" dmx-class:ds-setting="status==0">{{(status==1)?'Active':'InActive'}}</button></td>
                                <td dmx-text="fname+' '+lname"></td>
                                <td dmx-text="created_on.formatDate('dd-MM-yyyy hh:mm:ss A')"></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>